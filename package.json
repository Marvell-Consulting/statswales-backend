{
  "name": "statswales",
  "version": "1.0.0",
  "description": "This is the StatsWales private alpha repository.  This repository is here to allow us to experiment with a number of technologies to prove out a number of hypotheses and establish the technologies we may wish to take in to Beta.",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy-assets": "ts-node tools/copy-assets",
    "tsc": "tsc",
    "prettier:ci": "prettier --config .prettierrc src/**/*.ts --check",
    "prettier:fix": "prettier --config .prettierrc src/**/*.ts --write",
    "lint:ci": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm-run-all clean tsc copy-assets",
    "pretest": "bash -c 'f() { if [ $(command -v podman) ]; then podman compose up -d db-test; else docker compose up -d db-test; fi }; f'",
    "test": "jest --coverage",
    "test:ci": "jest --ci --coverage --config=jest.config.ts",
    "check": "npm-run-all prettier:fix lint:fix test build",
    "predev": "bash -c ' func() { if [ $(command -v podman) ]; then podman compose up -d db-dev redis; else docker compose up -d db-dev redis; fi }; func'",
    "dev:check": "npm-run-all check dev",
    "dev": "ts-node-dev --respawn src/server.ts | pino-colada",
    "start": "npm run build && node dist/server.js | pino-colada",
    "start:container": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm-seeding": "ts-node ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js",
    "migration:show": "npm run typeorm migration:show -- --dataSource=./src/db/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- --dataSource=./src/db/data-source.ts",
    "migration:drop": "npm run typeorm schema:drop -- --dataSource=./src/db/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- --dataSource=./src/db/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- --dataSource=./src/db/data-source.ts",
    "seed:required": "npm run typeorm-seeding -- -d ./src/db/data-source.ts ./src/seeders/*.ts",
    "seed:e2e": "npm run typeorm-seeding -- -d ./src/db/data-source.ts ./test/fixtures/seed-test-fixtures.ts",
    "seed:ci": "npm run typeorm-seeding -- -d ./src/db/data-source.ts ./test/fixtures/seed-test-fixtures.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@shopify/eslint-plugin": "^46.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.6",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/redis": "^4.0.10",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^6.0.2",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.1",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-junit-reporter": "^1.1.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^2.2.2",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "shelljs": "^0.8.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "@azure/identity": "^4.5.0",
    "@azure/storage-blob": "^12.26.0",
    "@azure/storage-file-datalake": "^12.25.0",
    "@jorgebodega/typeorm-seeding": "^7.1.0",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.1",
    "cookie-parser": "^1.4.7",
    "csv": "^6.3.11",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "duckdb-async": "^1.1.3",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "i18next": "^24.2.1",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.1",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.1.4",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "openid-client": "^5.7.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tmp": "^0.2.3",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  }
}
